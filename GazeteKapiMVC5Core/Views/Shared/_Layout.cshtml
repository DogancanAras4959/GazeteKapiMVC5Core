<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8">
    <title>GAZETE KAPI | YÖNETİM PANELİ</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Gazete Kapı Yönetim Paneliyle tüm haberlerinizi yönetin" name="description">
    <meta content="GKapı" name="author">

    <link rel="shortcut icon" href="https://uploads.gazetekapi.com/admin/wwwroot/favicon.ico">

    <link href="https://uploads.gazetekapi.com/admin/wwwroot/assets/libs/c3/c3.min.css" rel="stylesheet" type="text/css">
    <link href="https://uploads.gazetekapi.com/admin/wwwroot/assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css">
    <link href="https://uploads.gazetekapi.com/admin/wwwroot/assets/css/app.min.css" id="app-style" rel="stylesheet" type="text/css">
    <link href="~/assets/css/icons.min.css" rel="stylesheet" type="text/css">

</head>

<body data-sidebar="dark">
    <div id="layout-wrapper">

        @await Component.InvokeAsync("Header")

        @await Component.InvokeAsync("Sidebar")

        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
        </div>

    </div>

    <div class="rightbar-overlay"></div>

    <!-- JAVASCRIPT -->
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/libs/jquery/jquery.min.js"></script>
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/libs/metismenu/metisMenu.min.js"></script>
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/libs/simplebar/simplebar.min.js"></script>
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/libs/node-waves/waves.min.js"></script>
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/libs/jquery-knob/jquery.knob.min.js"></script>
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/js/pages/dashboard.init.js"></script>
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/js/app.js"></script>
    <script src="https://uploads.gazetekapi.com/admin/wwwroot/assets/libs/admin-resources/bootstrap-filestyle/bootstrap-filestyle.min.js"></script>
   
    <script type="text/javascript">
        function preventBack() { window.history.forward(); }
        setTimeout("preventBack()", 0);
        window.onunload = function () { null };
    </script>

    @RenderSection("Scripts", required: false)

</body>
</html>
